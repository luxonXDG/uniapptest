
._h5{
	text-align: center;
	font-size: 36rpx;
	font-weight: normal;
	padding: 40rpx 0;
	border-bottom: #808080 2rpx solid;
}
.title{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	padding: 30rpx 0;
}
.title text{
	font-size: 34rpx;
}
.uni-list radio-group{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	padding: 20rpx 100rpx;
}
.uni-list label{
	font-size: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}


