
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
}
.banner{
	width: 100%;
}
.banner image{
	width: 100%;
}
.shouye_btn{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-flex-wrap: wrap;
	        flex-wrap: wrap;
	width: 100%;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
}
.shouye_btn .shouye_btn_content{
	border: 2rpx #333333 solid;
	width: 48%;
	height: 120rpx;
	margin-top: 10rpx;
	text-align: center;
	line-height: 120rpx;
	font-size: 32rpx;
}
.main{
	width: 100%;
}
.main>view>text{
	font-size: 28rpx;
	color: #808080;
	margin-top: 20rpx;
}
.yongyao{
	padding: 0 5%;
}
.yongyaoContent{
	width: 100%;
	height: 180rpx;
	border: #808080 solid 2rpx;
	overflow: auto;
}
.myDoc{
	padding: 0 5%;
}
.myDocContent{
	width: 100%;
	height: 1000rpx;
	border: #808080 solid 2rpx;
}
.date{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-size: 26rpx;
	padding: 10rpx;
	font-weight: bold;
}
.yongyaoList{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	font-size: 24rpx;
	padding: 10rpx;
}
.yaoBtn{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	margin-top: 20rpx;
}
.head{
	padding: 30rpx 30rpx 0 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
}
.headImg image{
	width: 100rpx;
	height: 100rpx;
	border-radius: 50%;
	border: #333333 2rpx solid;
}
.headText{
	font-size: 30rpx;
	line-height: 50rpx;
	margin-left: 20rpx;
}
.headText text:nth-of-type(2){
	margin-left: 40rpx;
}
.headTitle{
	font-size: 30rpx;
	line-height: 50rpx;
	margin-left: 20rpx;
	margin-right: 20rpx;
}
.headTitleBar{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-justify-content: space-around;
	        justify-content: space-around;
	border-bottom: #808080 solid 2rpx;
}
.headTitleBar text:nth-of-type(1){
	color: #007AFF;
}
.main-content-zhenliao{
	font-size: 30rpx;
	padding: 20rpx 40rpx;
}
.main-content-zhenliao>view{
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	        justify-content: space-between;
	padding: 20rpx 0;
	border-bottom: #808080 2rpx solid;
}
.more{
	font-size: 30rpx;
	float: right;
	margin-right: 30rpx;
	color: #007AFF;
}

